@(user: Users, activeSince : String)<?xml version="1.0" encoding="utf-8"?>
<users>
    <user>
        <name>@user.getPersonalInformation().getFirstName() @user.getPersonalInformation().getLastName()</name>
        <age>@user.getPersonalInformation().getAge()</age>
        <gender>@user.getPersonalInformation().getGender()</gender>
        <email>@user.getPersonalInformation().getUserEmail()</email>
        <nickname>@user.getUserInformation().getNickname()</nickname>
        <activities>@user.getUserInformation().getActivitiesOfInterest()</activities>
        <about>@user.getUserInformation().getDetails()</about>
        <active_since>@activeSince</active_since>
    </user>
</users>
